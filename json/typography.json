"body {\n  background-color: var(--colors-white);\n  color: var(--colors-black);\n  font-family: var(--typography-font);\n  font-size: var(--typography-size);\n  line-height: var(--typography-line-height);\n}\n\ncode,\npre,\nkbd,\nsamp {\n  font-family: var(--typography-code);\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  line-height: 1.5em;\n  margin: 0;\n  margin-bottom: 0.5em;\n}\n\nh1,\n.h1 {\n  font-size: 2em;\n}\n\nh2,\n.h2 {\n  font-size: 1.5em;\n}\n\nh3,\n.h3 {\n  font-size: 1.3em;\n}\n\nh4,\n.h4 {\n  font-size: 1.15em;\n}\n\nh5,\n.h5 {\n  font-size: 1em;\n}\n\nh6,\n.h6 {\n  font-size: 0.75em;\n  text-transform: uppercase;\n}\n\np {\n  margin: 0 0 1em 0;\n}\n\nblockquote {\n  border-left: 0.1em solid var(--colors-light-gray);\n  margin: 1.5em 0;\n  padding: 0 1.25em 0 1.24em;\n}\n\nhr {\n  border-color: var(--colors-light-gray);\n  border-style: solid;\n  border-width: 0.1em;\n  margin: 2em 0;\n}\n\nol ol li {\n  list-style-type: lower-alpha;\n}\n\nol ol ol li {\n  list-style-type: lower-roman;\n}\n\nb,\nstrong {\n  font-weight: 700;\n}\n\nsmall {\n  font-size: 0.8em;\n  line-height: 1.2em;\n}\n\nsub,\nsup {\n  font-size: 0.6em;\n  line-height: 0;\n  position: relative;\n  vertical-align: baseline;\n}\n\nsup {\n  top: -0.5em;\n}"